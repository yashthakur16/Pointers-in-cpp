#include<iostream>
using namespace std;

void update(int n)
{
    n++;
}

void update2(int &n)
{
    n++;
}
int main()
{
    int n=5;

    cout<<"Before update : "<<n<<endl;
    update(n);
    cout<<"After update : "<<n<<endl;

    cout<<"Before update2 : "<<n<<endl;
    update2(n);
    cout<<"After update2 : "<<n<<endl;
}
